<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <!--if client login and transport text or file :
              maxBufferPoolSize value must be equal to or bigger than 0
              maxBufferSize value must be inside the range 1-2147483647(Int32.MaxValue), and it has to bigger than header and transport content
              maxReceivedMessageSize value must be inside the range 1-9223372036854775807(Int64.MaxValue == long) and seem to have nothing else limit-->
        <binding name="netTcpBinding" maxBufferPoolSize="500000" maxBufferSize="500000"
          maxReceivedMessageSize="500000">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://10.201.41.88:8732//WCFService/Service1/"
        binding="netTcpBinding" bindingConfiguration="netTcpBinding"
        contract="netTCPServiceReference.IService1" name="netTcpBinding" />
    </client>
  </system.serviceModel>
</configuration>